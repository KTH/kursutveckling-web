{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursutveckling-web",
  "version": "2.0.0",
  "description": "Public pages for course development information per course and its course rounds",
  "main": "app.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-web.git"
  },
  "scripts": {
    "preinstall": "npx npm-force-resolutions",
    "babel-not-used": "babel public/js/app -d dist/js ",
    "vendor": "parcel build ./public/js/vendor.js",
    "test": "NODE_ENV=test jest test/tests/*.js --silent",
    "test:watch": "NODE_ENV=test jest test/tests/*.js --watch",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production rm -rf dist && parcel build ./public/js/app/app.jsx && npm run vendor --preserve-comments",
    "build-dev-no-minify": "NODE_ENV=development rm -rf dist && parcel build --no-minify ./public/js/app/app.jsx && npm run vendor && parcel watch ./public/js/app/app.jsx",
    "build-dev": "cross-env NODE_ENV=development rm -rf dist && parcel build ./public/js/vendor.js --public-url /kursutveckling/static && parcel watch ./public/js/app/app.jsx --public-url /kursutveckling/static",
    "start": "NODE_ENV=production node app.js",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\"",
    "start-dev-win": "cross-env NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev-win\" \"nodemon --ignore dist/ --ignore public/ app.js\"",
    "build-dev-win": "cross-env NODE_ENV=development rimraf dist && parcel build ./public/js/vendor.js --public-url /kursutveckling/static && parcel watch ./public/js/app/app.jsx --public-url /kursutveckling/static"
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "@kth/kth-reactstrap": "^0.4.48",
    "applicationinsights": "^1.8.10",
    "axios": "^0.21.4",
    "body-parser": "^1.19.1",
    "component-registry": "^1.3.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "dotenv": "^8.2.0",
    "eslint-plugin-jest-extended": "0.0.1",
    "express": "^4.17.2",
    "express-handlebars": "^5.3.5",
    "kth-node-access-log": "^0.2.7",
    "kth-node-api-call": "^3.0.18",
    "kth-node-configuration": "^2.0.8",
    "kth-node-express-routing": "^2.0.12",
    "kth-node-i18n": "^1.0.12",
    "kth-node-log": "^3.1.11",
    "kth-node-monitor": "^1.1.14",
    "kth-node-redis": "^3.1.36",
    "kth-node-server": "^3.1.9",
    "kth-node-session": "^2.0.4",
    "kth-node-web-common": "^5.1.17",
    "kth-style": "^7.0.8",
    "locale": "^0.1.0",
    "mobx": "^5.15.6",
    "mobx-react": "^5.4.4",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-icons": "^4.3.1",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "reactstrap": "^8.10.1",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@kth/eslint-config-kth": "^1.0.15",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.3.6",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "merge-stream": "^2.0.0",
    "mockery": "^2.1.0",
    "nock": "^13.2.2",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.15",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.5.1",
    "prettier-eslint-cli": "^5.0.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^16.14.0",
    "sass": "^1.49.0"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        },
        "plugin-proposal-decorators"
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        },
        "plugin-proposal-class-properties"
      ],
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        },
        "plugin-proposal-private-methods"
      ],
      "@babel/plugin-transform-runtime"
    ]
  },
  "engines": {
    "node": "14.0.0"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}

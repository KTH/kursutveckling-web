{
  "name": "kursutveckling-web",
  "version": "2.0.0",
  "description": "Public pages for course development information per course and its course rounds",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-web.git"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\" --quiet",
    "lint-v": "eslint \"{public,server}/**/*.{js,jsx}\" ",
    "prepare": "husky install",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'"
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "@kth/api-call": "^4.0.39",
    "@kth/kth-node-response": "^1.0.7",
    "@kth/kth-node-web-common": "^7.0.12",
    "@kth/kth-reactstrap": "^0.4.54",
    "@kth/log": "^4.0.4",
    "@kth/monitor": "^3.0.25",
    "@kth/server": "^4.0.3",
    "@kth/session": "^3.0.4",
    "applicationinsights": "^2.3.4",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.29.1",
    "date-fns-tz": "^1.3.6",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "http-proxy-middleware": "^2.0.6",
    "kth-node-access-log": "^0.2.9",
    "kth-node-configuration": "^2.0.12",
    "kth-node-express-routing": "^2.0.15",
    "kth-node-i18n": "^1.0.14",
    "kth-node-redis": "^3.1.42",
    "kth-style": "^7.0.29",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.4.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "reactstrap": "^8.10.1",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.9",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@kth/eslint-config-kth": "^3.0.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^28.1.6",
    "ansi-regex": "^6.0.1",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "decache": "^4.6.1",
    "eslint": "^8.21.0",
    "eslint-plugin-jest-extended": "0.0.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jest-extended": "^3.0.1",
    "mini-css-extract-plugin": "^2.6.1",
    "mockery": "^2.1.0",
    "nodemon": "^2.0.19",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^2.7.1",
    "prettier-eslint-cli": "^6.0.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^17.0.2",
    "rimraf": "^3.0.2",
    "sass": "^1.54.5",
    "sass-loader": "^13.0.2",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "engines": {
    "node": "16"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  }
}

{
  "name": "kursutveckling-web",
  "version": "2.0.0",
  "description": "Public pages for course development information per course and its course rounds",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-web.git"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\" --quiet",
    "lint-v": "eslint \"{public,server}/**/*.{js,jsx}\" ",
    "prepare": "husky install",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'"
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "@kth/api-call": "^4.0.40",
    "@kth/kth-node-response": "^1.0.7",
    "@kth/kth-node-web-common": "^9.0.1",
    "@kth/kth-reactstrap": "^0.4.78",
    "@kth/log": "^4.0.7",
    "@kth/monitor": "^4.2.1",
    "@kth/server": "^4.0.4",
    "@kth/session": "^3.0.9",
    "applicationinsights": "^2.9.1",
    "axios": "^1.6.2",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-handlebars": "^7.1.2",
    "http-proxy-middleware": "^2.0.6",
    "kth-node-access-log": "^0.2.9",
    "kth-node-configuration": "^2.0.13",
    "kth-node-express-routing": "^2.0.16",
    "kth-node-i18n": "^1.0.18",
    "kth-node-redis": "^3.2.0",
    "kth-style": "^9.0.11",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-router": "^6.20.1",
    "react-router-dom": "^6.20.1",
    "reactstrap": "^9.2.1",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.4",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@babel/runtime": "^7.23.5",
    "@kth/eslint-config-kth": "^3.1.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.11",
    "ansi-regex": "^6.0.1",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "decache": "^4.6.2",
    "eslint": "^8.55.0",
    "eslint-plugin-jest-extended": "2.0.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.2.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mockery": "^2.1.0",
    "nodemon": "^3.0.2",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^3.1.0",
    "prettier-eslint-cli": "^8.0.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": "18"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npx prettier --write"
    ]
  }
}

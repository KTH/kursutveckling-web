{
  "//": "JSHint configuration - http://jshint.com/docs/options/",
  "name": "kursutveckling-web",
  "version": "2.0.0",
  "description": "Public pages for course development information per course and its course rounds",
  "main": "app.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-web.git"
  },
  "scripts": {
    "babel-not-used": "babel public/js/app -d dist/js ",
    "vendor": "parcel build ./public/js/vendor.js",
    "test": "mocha test/unit/*.js",
    "docker": "npm install --development && npm run build && npm prune --production",
    "build": "NODE_ENV=production parcel build ./public/js/app/app.jsx && npm run vendor --preserve-comments",
    "build-dev-no-minify": "NODE_ENV=development rm -rf dist && parcel build --no-minify ./public/js/app/app.jsx && npm run vendor && parcel watch ./public/js/app/app.jsx",
    "build-dev": "NODE_ENV=development rm -rf dist && parcel build ./public/js/app/app.jsx && npm run vendor && parcel watch ./public/js/app/app.jsx",
    "build-dev-old-gulp": "NODE_ENV=development rm -rf ./dist && gulp build && parcel build ./public/img/* && parcel build ./public/js/vendor.js && parcel watch ./public/js/app/app.jsx",
    "start": "NODE_ENV=production node app.js",
    "start-nodemon": "nodemon --ignore dist/ --ignore public/ app.js\"",
    "start-dev": "cross-env NODE_ENV=development concurrently --kill-others \"npm run build-dev\" \"nodemon --ignore dist/ --ignore public/ app.js\""
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "applicationinsights": "^1.7.6",
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "co": "^4.6.0",
    "component-registry": "^1.3.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-handlebars": "^4.0.4",
    "kth-node-access-log": "^0.2.0",
    "kth-node-api-call": "^3.0.11",
    "kth-node-configuration": "^2.0.1",
    "kth-node-express-routing": "^2.0.3",
    "kth-node-i18n": "^1.0.2",
    "kth-node-ldap": "^3.1.1",
    "kth-node-log": "^3.1.0",
    "kth-node-monitor": "^1.1.3",
    "kth-node-passport-cas": "^3.1.11",
    "kth-node-redis": "^3.1.6",
    "kth-node-server": "^3.1.2",
    "kth-node-session": "^1.0.4",
    "kth-node-web-common": "^4.0.2",
    "kth-style": "^4.2.17",
    "locale": "^0.1.0",
    "mobx": "^4.13.0",
    "mobx-react": "^5.4.3",
    "passport": "^0.4.0",
    "popper.js": "^1.14.5",
    "prop-types": "^15.7.2",
    "query-string": "^5.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "reactstrap": "^7.1.0",
    "safe-utils": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "^23.9.0",
    "eslint-plugin-jsx-a11y": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.1",
    "husky": "^1.3.1",
    "merge-stream": "^1.0.1",
    "mocha": "5.2.0",
    "mockery": "^2.1.0",
    "nock": "^10.0.2",
    "node-mocks-http": "^1.7.3",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.7",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.16.4",
    "sass": "^1.17.3",
    "zombie": "6.1.3"
  },
  "engines": {
    "node": "8.12.0"
  },
  "jshintConfig": {
    "maxerr": 5
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        },
        "plugin-proposal-decorators"
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        },
        "plugin-proposal-class-properties"
      ]
    ]
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "semi": false,
    "trailingComma": "none",
    "tabWidth": 2
  }
}

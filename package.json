{
  "name": "kursutveckling-web",
  "version": "2.0.0",
  "description": "Public pages for course development information per course and its course rounds",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-web.git"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\" --quiet",
    "lint-v": "eslint \"{public,server}/**/*.{js,jsx}\" ",
    "prepare": "husky install",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'"
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "@kth/kth-node-web-common": "^7.0.9",
    "@kth/kth-reactstrap": "^0.4.50",
    "applicationinsights": "^1.8.10",
    "axios": "^0.21.4",
    "body-parser": "^1.19.1",
    "component-registry": "^1.3.1",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "dotenv": "^8.2.0",
    "eslint-plugin-jest-extended": "0.0.1",
    "express": "^4.17.2",
    "express-handlebars": "^5.3.5",
    "kth-node-access-log": "^0.2.7",
    "kth-node-api-call": "^3.0.18",
    "kth-node-configuration": "^2.0.8",
    "kth-node-express-routing": "^2.0.12",
    "kth-node-i18n": "^1.0.12",
    "kth-node-log": "^3.1.11",
    "kth-node-monitor": "^1.1.14",
    "kth-node-redis": "^3.1.36",
    "kth-node-server": "^3.1.9",
    "kth-node-session": "^2.0.4",
    "kth-style": "^7.0.18",
    "locale": "^0.1.0",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.3.0",
    "react-icons": "^4.3.1",
    "react-router": "^5.2.1",
    "reactstrap": "^8.10.1",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "7.13.8",
    "@babel/preset-react": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@kth/eslint-config-kth": "^1.0.15",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^11.2.7",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.4",
    "chai": "^4.3.6",
    "concurrently": "^6.5.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "file-loader": "^6.2.0",
    "husky": "^5.1.3",
    "jest": "^26.6.3",
    "jest-extended": "^0.11.5",
    "merge-stream": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mockery": "^2.1.0",
    "nock": "^13.2.2",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.15",
    "null-loader": "^4.0.1",
    "prettier": "^2.5.1",
    "prettier-eslint-cli": "^5.0.1",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.49.0",
    "sass-loader": "^12.6.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2"
  },
  "resolutions": {
    "node-forge": "0.10.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "jest": {
    "clearMocks": true,
    "notifyMode": "failure-change",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "jsdom",
    "verbose": true
  }
}

{
  "name": "kursutveckling-web",
  "version": "2.0.0",
  "description": "Public pages for course development information per course and its course rounds",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-web.git"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\" --quiet",
    "lint-v": "eslint \"{public,server}/**/*.{js,jsx}\" ",
    "prepare": "husky install",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'"
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "@kth/kth-reactstrap": "^0.4.50",
    "@kth/api-call": "^4.0.37",
    "@kth/kth-node-response": "^1.0.5",
    "@kth/kth-node-web-common": "^7.0.10",
    "@kth/log": "^4.0.4",
    "@kth/monitor": "^3.0.24",
    "@kth/server": "^4.0.2",
    "@kth/session": "^3.0.3",
    "compression": "^1.7.3",
    "eslint-plugin-jest-extended": "0.0.1",
    "locale": "^0.1.0",
    "prop-types": "^15.8.1",
    "safe-utils": "^1.0.1",    
    "applicationinsights": "^2.3.1",
    "axios": "^0.27.2",
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-handlebars": "^6.0.5",
    "http-proxy-middleware": "^2.0.6",
    "kth-node-access-log": "^0.2.8",
    "kth-node-configuration": "^2.0.11",
    "kth-node-express-routing": "^2.0.13",
    "kth-node-i18n": "^1.0.13",
    "kth-node-redis": "^3.1.40",
    "kth-style": "^7.0.26",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "reactstrap": "^9.0.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@kth/eslint-config-kth": "^3.0.10",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@types/jest": "^27.4.1",
    "ansi-regex": "^6.0.1",
    "babel-jest": "^28.0.1",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "decache": "^4.6.1",
    "eslint": "^8.14.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "mini-css-extract-plugin": "^2.6.0",
    "nodemon": "^2.0.15",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^2.6.1",
    "prettier-eslint-cli": "^5.0.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",    
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-decorators": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.6",
    "mockery": "^2.1.0",
    "nock": "^13.2.2",
    "node-mocks-http": "^1.11.0",
    "react-test-renderer": "^17.0.2"
  },
  "engines": {
    "node": "16"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "jest": {
    "clearMocks": true,
    "notifyMode": "failure-change",
    "setupFilesAfterEnv": [
      "jest-extended"
    ],
    "testEnvironment": "jsdom",
    "verbose": true
  }
}

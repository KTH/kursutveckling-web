{
  "name": "kursutveckling-web",
  "version": "2.0.0",
  "description": "Public pages for course development information per course and its course rounds",
  "license": "MIT",
  "repository": {
    "type": "git",
    "uri": "git+https://github.com/KTH/kursutveckling-web.git"
  },
  "scripts": {
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:notify": "jest --watch --notify",
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\" --quiet",
    "lint-v": "eslint \"{public,server}/**/*.{js,jsx}\" ",
    "prepare": "husky install",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'"
  },
  "pre-push": [
    "test"
  ],
  "dependencies": {
    "@kth/api-call": "^4.4.0",
    "@kth/appinsights": "^0.6.0",
    "@kth/cortina-block": "^7.1.1",
    "@kth/kth-node-response": "^1.0.8",
    "@kth/kth-node-web-common": "9.6.1",
    "@kth/kth-reactstrap": "^0.5.0",
    "@kth/log": "^4.0.7",
    "@kth/monitor": "^4.3.1",
    "@kth/om-kursen-ladok-client": "^2.4.0",
    "@kth/server": "^4.1.0",
    "@kth/session": "^3.0.9",
    "@kth/style": "^1.13.0",
    "axios": "^1.12.2",
    "body-parser": "^1.20.3",
    "cookie-parser": "^1.4.7",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.1",
    "dotenv": "^16.6.1",
    "express": "^4.21.2",
    "express-handlebars": "^7.1.3",
    "http-proxy-middleware": "^2.0.9",
    "kth-node-access-log": "^0.2.10",
    "kth-node-configuration": "^2.1.0",
    "kth-node-express-routing": "^2.3.0",
    "kth-node-i18n": "^1.0.18",
    "kth-node-redis": "^3.4.0",
    "kth-style": "^10.5.3",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^4.12.0",
    "react-router": "^6.30.1",
    "react-router-dom": "^6.30.1",
    "reactstrap": "^9.2.3",
    "safe-utils": "^1.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@babel/runtime": "^7.28.4",
    "@kth/eslint-config-kth": "^3.6.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.18.6",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.8",
    "ansi-regex": "^6.2.2",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.11.0",
    "decache": "^4.6.2",
    "eslint": "^8.57.0",
    "eslint-plugin-jest-extended": "2.4.0",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.5.2",
    "mini-css-extract-plugin": "^2.9.4",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.1.10",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "prettier-eslint-cli": "^8.0.1",
    "react-test-renderer": "^18.3.1",
    "sass": "^1.93.2",
    "sass-loader": "^14.2.1",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4"
  },
  "engines": {
    "node": ">=20"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test"
    }
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 120
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npx prettier --write"
    ]
  }
}

            "KURS_PM_DATA_API_KEY",
            "SESSION_SECRET",
            "SESSION_KEY",
            "REDIS_URI",
            "APPINSIGHTS_INSTRUMENTATIONKEY"             
        ]
      },
      "environmentVariables": {
        "value": {
